@model Lab_Activity_2.Models.Instructor

<h2>Instructor Details</h2>

<div>
    <dl class="dl-horizontal">
        <dt>ID</dt>
        <dd>@Model.Id</dd>
        <dt>First Name</dt>
        <dd>@Model.FirstName</dd>
        <dt>Last Name</dt>
        <dd>@Model.LastName</dd>
        <dt>Tenure Status</dt>
        <dd>@Model.TenureStatus</dd>
        <dt>Rank</dt>
        <dd>@Model.Rank</dd>
        <dt>Hiring Date</dt>
        <dd>@Model.HiringDate.ToShortDateString()</dd>
        <dt>Email</dt>
        <dd>@Model.Email</dd>
    </dl>
</div>

<p>
    <a asp-action="Index">Back to List</a>
</p>

